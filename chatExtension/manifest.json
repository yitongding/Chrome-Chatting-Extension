{
	"name": "popup_chat",
	"manifest_version": 2,
	"description": "popup_chat",
	"version": "1.0",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["jquery.js","script.js"]
	}],
	"permissions": [
		"tabs",
		"activeTab",
    	"declarativeContent"
	]
}